    "ts-node-dev": "^1.1.1",
    "tsutils": "^3.20.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/main.ts",
